    <script>
      var swiper = new Swiper(".mySwiper", {
        slidesPerView: 1,
        spaceBetween: 0,
        loop: true,
        autoHeight: true,
        autoplay: {
          delay: 2500,
          disableOnInteraction: false,
        },

        breakpoints: {
          // when window width is >= 992px
          992: {
            slidesPerView: 2,
            spaceBetween: 0,
          },
        },
      });

      var swiper = new Swiper(".zoomSwiper", {
        slidesPerView: 1,
        spaceBetween: 20,
        loop: true,
        autoHeight: true,
        autoplay: {
          delay: 3000,
          disableOnInteraction: true,
        },
        pagination: {
          el: ".swiper-pagination",
        },

        breakpoints: {
          // when window width is >= 992px
          992: {
            slidesPerView: 3,
            spaceBetween: 1,
          },
        },
      });

      $(".counter").countUp();
      $(".counter2").countUp();
      $(".counter3").countUp();
    </script>

